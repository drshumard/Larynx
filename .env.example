# Larynx TTS Environment Configuration
# Copy this file to .env and fill in your values

# ElevenLabs API Configuration (Required)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# ElevenLabs Voice Settings (Optional - defaults provided)
ELEVENLABS_VOICE_ID=LNHBM9NjjOl44Efsdmtl
ELEVENLABS_MODEL=eleven_multilingual_v2

# Backend URL for frontend to connect to
# For local development: http://localhost:8001
# For production with nginx: leave empty or set to your domain
REACT_APP_BACKEND_URL=http://localhost:8001

# Webhook URL for job completion notifications (Optional)
WEBHOOK_URL=

# MongoDB Configuration (Optional - defaults to docker service)
# MONGO_URL=mongodb://mongodb:27017
# DB_NAME=tts_chunker
